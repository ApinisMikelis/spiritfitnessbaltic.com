{
  "name": "@mono/ui",
  "version": "0.0.0",
  "main": "/components/index.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./styles.css": "./dist/index.css",
    "./styles": "./dist/index.css",
    "./*": "./dist/*.js"
  },
  "license": "MIT",
  "scripts": {
    "build": "pnpm build:styles && pnpm build:components",
    "build:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "build:components": "tsup",
    "check-types": "tsc --noEmit",
    "dev:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "dev:components": "tsup --watch",
    "lint": "eslint src --max-warnings 0"
  },
  "peerDependencies": {
    "react": "^19"
  },
  "devDependencies": {
    "@mono/eslint-config": "workspace:*",
    "@mono/tailwind-config": "workspace:*",
    "@mono/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.17",
    "@types/react": "^19.2.2",
    "clsx": "^2.1.1",
    "eslint": "^9.39.1",
    "react-aria-components": "^1.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "libphonenumber-js": "^1.12.26"
  }
}
